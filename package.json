{
  "name": "@gummi-io/react-native-api-manager",
  "version": "1.0.0",
  "description": "A powerful, modern, and flexible API management library for React and React Native applications with centralized client management, state synchronization, and navigation integration.",
  "main": "dist/index.js",
  "module": "dist/index.mjs",
  "types": "dist/index.d.ts",
  "files": [
    "dist",
    "README.md",
    "WARP.md"
  ],
  "scripts": {
    "build": "tsup src/index.js --format cjs,esm --dts",
    "dev": "tsup src/index.js --format cjs,esm --dts --watch",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "lint": "eslint src --ext .js,.jsx",
    "fix": "eslint src --ext .js,.jsx --fix",
    "format": "prettier --write src/**/*.{js,jsx}",
    "format:check": "prettier --check src/**/*.{js,jsx}",
    "clean": "rimraf dist",
    "prepublishOnly": "npm run clean && npm run build && npm run test"
  },
  "keywords": [
    "react-native",
    "api-client"
  ],
  "author": {
    "name": "Gummi IO",
    "email": "welp@gummi.io"
  },
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/Silas-and-Sobae-Gummi-IO/React-Native-Api-Manager.git"
  },
  "peerDependencies": {
    "@react-navigation/native": ">=6.0.0",
    "react": ">=18.0.0"
  },
  "peerDependenciesMeta": {
    "@react-navigation/native": {
      "optional": true
    }
  },
  "devDependencies": {
    "@babel/core": "^7.28.4",
    "@babel/preset-env": "^7.28.3",
    "@babel/preset-react": "^7.27.1",
    "@eslint/js": "^9.35.0",
    "@react-native/babel-preset": "^0.81.1",
    "@testing-library/jest-dom": "^6.8.0",
    "@testing-library/react": "^16.3.0",
    "@testing-library/react-native": "^13.3.3",
    "babel-jest": "^30.1.2",
    "eslint": "^9.35.0",
    "eslint-config-prettier": "^10.1.8",
    "eslint-plugin-react": "^7.37.5",
    "eslint-plugin-react-hooks": "^5.2.0",
    "jest": "^30.1.3",
    "jest-environment-jsdom": "^30.1.2",
    "prettier": "^3.6.2",
    "react": "^19.1.1",
    "react-native": "^0.81.4",
    "react-test-renderer": "^19.1.1",
    "rimraf": "^6.0.1",
    "tsup": "^8.5.0",
    "typescript": "^5.9.2"
  }
}
